{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Select20 Backend in Laravel",
    "keywords": ["Select20"],
    "license": "GPLv2",
    "require": {
        "php": "^8.2",
        "comcast/php-legal-licenses": "^1.2",
        "darkaonline/l5-swagger": "^8.5.1",
        "dominikb/composer-license-checker": "^2.4.2",
        "enlightn/enlightn": "^2.5",
        "fideloper/proxy": "^4.4.2",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.8.0",
        "laravel/framework": "^9.52.15",
        "laravel/passport": "^10.4.2",
        "laravel/tinker": "^2.8.2",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "enlightn/laravel-security-checker": "^1.1",
        "spatie/laravel-ignition": "^1.6.4",
        "fakerphp/faker": "^1.23.0",
        "laravel/sail": "^1.25.0",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^6.4",
        "phpunit/phpunit": "^9.6.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
